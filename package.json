{
  "name": "video-transcription",
  "version": "1.0.0",
  "templatetype": "microservice",
  "templateversion": "1.0.0",
  "description": "Microservice for transcribing YouTube videos using Google Cloud Speech-to-Text or OpenAI Whisper",
  "main": "app/index.ts",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only ./app/index.ts",
    "start:dev": "ts-node-dev --respawn --transpileOnly ./app/index.ts",
    "start:watch": "ts-node-dev --respawn ./app/index.ts",
    "start:debug": "ts-node --inspect ./app/index.ts",
    "build": "tsc --module commonjs",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "lint": "eslint app/**/*.ts --color --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
