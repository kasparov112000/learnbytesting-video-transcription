    "@types/node": "^18.19.0",
    "awilix": "^4.2.6",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "bson-objectid": "^1.3.0",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-unless": "^2.1.3",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "googleapis": "~166.0.0",
